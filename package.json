{
  "name": "bundle-loader-easy",
  "version": "1.0.0",
  "description": "this is a component to easy lazy load react-component,it's rely on bundle-loader",
  "main": "dist/index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && rollup -c rollup.conf.js --watch",
    "build": "export NODE_ENV=production && rollup -c rollup.conf.js && uglifyjs -c -m -o dist/index.min.js -- dist/useful-index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clerkkent/bundle-loader-easy.git"
  },
  "keywords": [
    "react",
    "按需加载方案",
    "react-router4.0",
    "bundle-loader"
  ],
  "author": {
    "name": "clerkkent",
    "email": "458181109@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clerkkent/bundle-loader-easy/issues"
  },
  "homepage": "https://github.com/clerkkent/bundle-loader-easy#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "rollup": "^0.51.5",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "uglify-js": "^3.1.9"
  },
  "dependencies": {
    "bundle-loader": "^0.5.6",
    "prop-types": "^15.6.1",
    "react": "^16.2.0"
  }
